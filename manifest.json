{
    "manifest_version": 3,
    "name": "Literate Octo Winner",
    "version": "1.0.0",
    "description": "Literate Octo Winner desc",
    "content_scripts": [
        {
            "js": [
                "main.js"
            ],
            "matches": [
                "https://*/*"
            ]
        }
    ],
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
        "16": "images/lo_icon_16.png",
        "32": "images/lo_icon_32.png",
        "48": "images/lo_icon_48.png",
        "128": "images/lo_icon_128.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
}